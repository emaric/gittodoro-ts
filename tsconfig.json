{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/interactor/*": ["interactor/*"],
      "@/examples/*": ["examples/*"]
    },
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "lib",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["interactor", "examples"]
}
